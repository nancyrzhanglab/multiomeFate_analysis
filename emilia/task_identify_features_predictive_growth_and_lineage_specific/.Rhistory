# ==============================================================================
# Read data
# ==============================================================================
in_dir <- "~/Dropbox/Thesis/Lineage_trace/data/Minn_lab/SCP9_11_tumor/"
tumor_dat.darwin <- read.csv(paste0(in_dir, 'Darwin Res 499 SCP 11 vs SCP 9 Chronic IFNG Tumor Growth.csv'))
metadat <- read.csv(paste0(in_dir, 'Darwin Res 499 SCP 11 vs SCP 9 Chronic IFNG Tumor Growth Metadata.csv'))
tumor_dat.nathan <- read.csv(paste0(in_dir, 'Nathan_SCP9_SCP11_chronic_gamma_ICB_long.csv'))
colnames(tumor_dat.nathan) <- c("Mouse","Clone", "Condition", "Treatment", "Time", "Tumor volume",  "Event")
# ==============================================================================
tumor_dat.darwin.long <- as.data.frame(t(tumor_dat.darwin))
colnames(tumor_dat.darwin.long) <- tumor_dat.darwin.long['Day', ]
tumor_dat.darwin.long <- tumor_dat.darwin.long[!(row.names(tumor_dat.darwin.long) %in% c('Day')),]
tumor_dat.darwin.long$Mouse <- rownames(tumor_dat.darwin.long)
tumor_dat.darwin.long <- melt(tumor_dat.darwin.long, ids = 'Mouse')
colnames(tumor_dat.darwin.long) <- c('Mouse', 'day', 'Tumor volume')
tumor_dat.darwin.long <- merge(tumor_dat.darwin.long, metadat, by.x = 'Mouse', by.y = 'ID')
tumor_dat.darwin.long$Gamma <- ifelse(grepl('y', tumor_dat.darwin.long$SampleID), 'gamma', 'mock')
tumor_dat.darwin.long[, c('Clone', 'Name')] <- str_split_fixed(tumor_dat.darwin.long$SampleID, '_', 2)
tumor_dat.darwin.long$Clone <- ifelse(grepl('9', tumor_dat.darwin.long$Clone), '9', '11')
tumor_dat.darwin.long$ICB <- ifelse(grepl('ICB', tumor_dat.darwin.long$SampleID), 'ICB', 'control')
tumor_dat.darwin.long$`Tumor volume mm3` <- tumor_dat.darwin.long$`Tumor volume` * 1000
tumor_dat.darwin.long$Clone <- as.factor(tumor_dat.darwin.long$Clone)
tumor_dat.darwin.long$ICB <- as.factor(tumor_dat.darwin.long$ICB)
tumor_dat.darwin.long <- tumor_dat.darwin.long[, c('Mouse', 'day', 'Tumor volume mm3', 'Gamma', 'ICB', 'Clone')]
colnames(tumor_dat.darwin.long) <- c('Mouse', 'Day', 'Tumor volume', 'Gamma', 'ICB', 'Clone')
tumor_dat.darwin.long$Day <- as.numeric(as.character(tumor_dat.darwin.long$Day))
# ==============================================================================
# Process Nathan's data
# ==============================================================================
temp <- tumor_dat.nathan %>%
as_tibble() %>%
dplyr::filter(!is.na(`Tumor volume`)) %>%
dplyr::mutate(day = as.numeric(sub("D", "", Time)),
drug = as.factor(Condition),
ICB = as.factor(Treatment)) %>%
dplyr::mutate(drug = dplyr::recode(Condition, 'mock' = 'mock'),
group = factor(paste(ICB, drug, sep = "_")),
vol.log = log10(`Tumor volume`))
tumor_dat.nathan.long <- temp[, c('Mouse', 'day', 'Tumor volume', 'drug', 'ICB', 'Clone')]
colnames(tumor_dat.nathan.long) <- c('Mouse', 'Day', 'Tumor volume', 'Gamma', 'ICB', 'Clone')
tumor_dat <- rbind(tumor_dat.darwin.long, tumor_dat.nathan.long)
tumor_dat$vol.log = log10(tumor_dat$`Tumor volume` + 1e-5)
tumor_dat$Mouse <- as.factor(tumor_dat$Mouse)
